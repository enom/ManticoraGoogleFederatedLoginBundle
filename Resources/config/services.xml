<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <parameters>
        <parameter key="manticora_google_federated_login.relay_party.class">Manticora\GoogleFederatedLoginBundle\Security\RelayParty\RelayParty</parameter>
        <parameter key="manticora_google_federated_login.domain"/>
    </parameters>

    <services>


        <service id="manticora_google_federated_login.relay_party" class="%manticora_google_federated_login.relay_party.class%">
            <argument>%manticora_google_federated_login.domain%</argument>
        </service>
    </services>

</container>
